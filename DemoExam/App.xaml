<Application x:Class="DemoExam.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DemoExam"
             StartupUri="/Views/Auth.xaml"
             >
    <Application.Resources>
        <local:DiscountToBGConverter x:Key="DiscountToBGConverter"/>
        <local:QuantityToColorConverter x:Key="QuantityToColorConverter"/>
        <local:OrderIdToVisibilityConverter x:Key="OrderIdToVisibilityConverter"/>
        <local:DateToDatePickerConverter x:Key="DateToDatePickerConverter"/>
        <local:RadioButtonConverter x:Key="RadioButtonConverter"/>
        <local:FiltersVisibilityConverter x:Key="FiltersVisibilityConverter"/>
        <local:TovarIdVisibility x:Key="TovarIdVisibility"/>
        <local:TovarDeleteVisibility x:Key="TovarDeleteVisibility"/>
        <local:ManagerVisibilityConverter x:Key="ManagerVisibilityConverter"/>
        <local:PriceVisibilityConverter x:Key="PriceVisibilityConverter"/>
        <local:DiscountMathConverter x:Key="DiscountMathConverter"/>
        <FontFamily x:Key="TimesNewRoman"/>
    </Application.Resources>
</Application>
